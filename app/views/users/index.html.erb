<div class="row">
  <div class="col-sm-12">
    <h3>Manage Partners</h3>

      <div class="table-responsive">
        <table class="table table-striped" id="table-partners">
          <thead>
            <th>Name</th>
            <th>Email</th>
            <th width="210">Actions</th>
          </thead>
          <tbody>
            <%= render @partners %>
            <%= render "users/form", object: User.new, action: 'post', button_name: "Add Partner" %>
          </tbody>
        </table>
        <div id='partner-errors' class='form-errors'>
        </div>
      </div>
  </div>
</div>
