<h3>Partners</h3>

<%= link_to excel_users_path(format: :xlsx), class: 'btn btn-light' do %>
  <i class="bi bi-file-earmark-excel"></i> Export to Excel
<% end %>

<span class="table">
  <span class="thead">
    <span class="tr">
      <span class="th">Name</span>
      <span class="th">Email</span>
      <span class="th">Actions</span>
    </span>
  </span>
  <span id="partners" class="tbody">
    <%= render @partners %>
  </span>
  <span class="tfoot">
    <%= render partial: 'new_partner', locals: {object: User.partners.build} %>
  </span>
</span>
